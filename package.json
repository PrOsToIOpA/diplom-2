{
  "name": "raboom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/",
    "fix": "yarn lint --fix",
    "ts": "yarn run tsc",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean --force && yarn install && yarn start -- --reset-cache",
    "reinstall": "rm -rf node_modules && cd ios && rm -rf Pods && cd .. && yarn && cd ios && pod install",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-clipboard/clipboard": "^1.8.4",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-firebase/analytics": "^12.4.0",
    "@react-native-firebase/app": "^12.4.0",
    "@react-native-firebase/messaging": "^13.0.1",
    "@react-navigation/bottom-tabs": "^5.11.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "@sentry/react-native": "^3.1.1",
    "@types/i18n-js": "^3.8.2",
    "@types/lodash.memoize": "^4.1.6",
    "apisauce": "^2.1.1",
    "axios": "^0.21.1",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "lodash.memoize": "^4.1.2",
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-native": "0.64.2",
    "react-native-branch": "^5.0.0",
    "react-native-date-picker": "^4.1.3",
    "react-native-fast-image": "^8.3.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localize": "^2.1.4",
    "react-native-modal": "^12.0.2",
    "react-native-permissions": "^3.0.5",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-share": "^7.0.0",
    "react-native-svg": "^12.1.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-swiper-flatlist": "^3.0.14",
    "react-native-video": "^5.1.1",
    "react-native-webview": "^11.6.5",
    "react-redux": "^7.2.4",
    "redux-devtools-extension": "^2.13.9",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "retry-axios": "^2.4.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/runtime": "^7.14.6",
    "@react-native-community/eslint-config": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.13",
    "@types/react-native": "^0.64.10",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.0.6",
    "eslint": "^7.29.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn fix"
  },
  "jest": {
    "preset": "react-native"
  }
}
